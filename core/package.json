    "moment-timezone": "^0.5.13",
    "node-sass-chokidar": "^0.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-big-calendar": "^0.16.1",
    "react-datepicker": "^0.55.0",
    "react-dom": "^16.0.0",
    "react-google-maps": "^9.0.2",
    "react-image-lightbox": "^4.3.0",
