{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "anouar/fpdf": "^2.0",
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "v0.11.2",
        "caffeinated/modules": "v4.3.2",
        "damnyan/cmn": "v4.1.2",
        "fideloper/proxy": "4.1.0",
        "guzzlehttp/guzzle": "6.3.3",
        "kreait/firebase-php": "4.18.2",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "v1.0.8",
        "nicopenaredondo/multipay": "v1.4",
        "owen-it/laravel-auditing": "v8.0.4",
        "tymon/jwt-auth": "1.0.0-rc.2",
        "emadadly/laravel-uuid": "1.*"
    },
    "require-dev": {
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "v1.8.0",
        "mockery/mockery": "1.2.0",
        "nunomaduro/collision": "v2.1.1",
        "phpunit/phpunit": "7.5.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
